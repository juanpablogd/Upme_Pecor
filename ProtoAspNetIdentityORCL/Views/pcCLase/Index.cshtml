@model IEnumerable<NSPecor.Models.MUB_CLASE_CP>

@{
    ViewBag.Title = "Clase";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Listado de Clase de Proyecto</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NOM_CLASE_CP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_ACTUALIZACION)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NOM_CLASE_CP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHA_ACTUALIZACION)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_CLASE_CP }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.ID_CLASE_CP }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_CLASE_CP })
        </td>
    </tr>
}

</table>
